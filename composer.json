{
  "name": "niden/cardoe",
  "description": "ADR Test Bed",
  "keywords": [
    "php",
    "framework",
    "psr-3",
    "psr-4",
    "psr-7",
    "psr-11",
    "psr-13",
    "psr-14",
    "psr-15",
    "psr-16",
    "psr-17",
    "psr-18"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3 <8.0",
    "ext-apcu": "*",
    "ext-igbinary": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-pdo": "*",
    "ext-xml": "^7.2",
    "pds/skeleton": "@dev",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/link": "^1.0",
    "psr/log": "^1.1",
    "psr/simple-cache": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Phalcon\\": "src/",
      "Phalcon\\Test\\Unit\\": "tests/unit/",
      "Phalcon\\Test\\Fixtures\\": "tests/_data/fixtures/"
    }
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "codeception/codeception": "^3.1",
    "phpstan/phpstan": "^0.11.19",
    "squizlabs/php_codesniffer": "^3.4",
    "vlucas/phpdotenv": "^4.0"
  }
}
